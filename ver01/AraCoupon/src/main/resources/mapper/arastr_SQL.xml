<!DOCTYPE mapper PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="AraStr">

	<!-- ////////////////////////////////////////////////////////////////////////////////// -->
	<!-- ////////////////////////////////////////////////////////////////////////////////// -->
	<!-- ////////////////////////////////////////////////////////////////////////////////// -->
	<!-- ////////////////////////////////////////////////////////////////////////////////// -->
	<!-- ////////////////////////////////////////////////////////////////////////////////// -->
	<!-- ////////////////////////////////////////////////////////////////////////////////// -->

	<!-- select store information -->
	<select id="AraStr.selectStoreInfo" parameterType="Map" resultType="Map">
		SELECT  /* AraStr.selectStoreInfo */
			A.STR_ID
			, A.STR_NM
			, A.MBL_NO
		FROM
			ARA_STR  A
		WHERE 1=1
			AND A.STR_ID = #{strid}
	</select>

</mapper>
