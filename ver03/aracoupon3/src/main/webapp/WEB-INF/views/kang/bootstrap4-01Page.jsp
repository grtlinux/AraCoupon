	<div class="container-fluid show">
		<h2>DIV as Table</h2>
		<div class="d-table bg-warning" style="width:100%">
			<div class="d-table-row" style="width:100%">
	
				<div class="d-sm-none d-table-cell">
					<h4>Left Cell</h4>
					<p>Some text..</p>
				</div>
	
				<div class="d-sm-none d-table-cell">
					<h4>Middle Cell</h4>
					<p>Some text..</p>
				</div>
	
				<div class="d-table-cell">
					<h4>Right Cell</h4>
					<p>Some text..</p>
				</div>
	
			</div>
		</div>
		
		<br>
		<h2>Responsive Table</h2>
		<div class="d-sm-table bg-success" style="width:100%">
			<div class="d-sm-table-row" style="width:100%">
	
				<div class="d-xs-table-cell">
					<h4>Left Cell</h4>
					<p>Some text..0</p>
				</div>
	
				<div class="d-none d-xs-table-cell">
					<h4>Left Cell</h4>
					<p>Some text..1</p>
				</div>
	
				<div class="d-none d-sm-table-cell">
					<h4>Middle Cell</h4>
					<p>Some text..2</p>
				</div>
	
				<div class="d-none d-sm-table-cell">
					<h4>Middle Cell</h4>
					<p>Some text..3</p>
				</div>
	
				<div class="d-none d-md-table-cell">
					<h4>Right Cell</h4>
					<p>Some text..4</p>
				</div>
	
				<div class="d-none d-md-table-cell">
					<h4>Right Cell</h4>
					<p>Some text..5</p>
				</div>
	
				<div class="d-none d-lg-table-cell">
					<h4>Right Cell</h4>
					<p>Some text..6</p>
				</div>
	
				<div class="d-none d-xl-table-cell">
					<h4>Right Cell</h4>
					<p>Some text..7</p>
				</div>
	
			</div>
		</div>

	</div>

